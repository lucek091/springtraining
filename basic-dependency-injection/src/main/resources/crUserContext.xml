<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="userCreator" class="info.spring.tutorial.UserCreator"></bean>
	<bean id="someUser" factory-bean="userCreator" factory-method="createUser">
		<constructor-arg value="Jan"></constructor-arg>
		<constructor-arg value="14"></constructor-arg>
	</bean>



</beans>
