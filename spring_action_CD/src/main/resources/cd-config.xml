<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="compactdisc" class="com.springaction.soundsystem.BlankDisc">
		<property name="title" value="BLANK DISC nowa plyta z konstruktora XML"></property>
		<property name="artist" value="BLANK DISC AUTOR XML"></property>
		<property name="tracks">
			<list>
				<value>Piosenka numer 1</value>
				<value>Piosenka numer 2</value>
				<value>Piosenka numer 3</value>

			</list>
		</property>

	</bean>

</beans>
